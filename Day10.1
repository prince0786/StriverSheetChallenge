## Linked List Cycle ll

Node *firstNode(Node *head)
{
	//    Write your code here.
    if(head == NULL || head->next == NULL)
        return NULL;
    Node *slow = head;
    Node *fast = head;
    Node *mani = head;
    
    while(fast->next != NULL && fast->next->next != NULL){
        slow = slow->next;
        fast = fast->next->next;
        if(slow == fast){
            while(slow != mani){
                slow = slow->next;
                mani = mani->next;
            }
            return mani;
        }
    }
    return NULL;
}
