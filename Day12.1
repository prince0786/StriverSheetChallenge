## Longest Common Prefix

string longestCommonPrefix(vector<string> &arr, int n)
{
    // Write your code here
    string fun = "";
    
    for(int i=0; i<arr[0].length(); i++){
        char ch = arr[0][i];
        bool match = true;
        for(int j=1; j<n; j++){
            if(ch==arr[j][i])
                match = true;
            else{
                match = false;
                break;
            }
        }
        if(match==true){
            fun.push_back(ch);
            match = true;
        }
        else
            break;
    }
    return fun;
}


