## Missing and Repeating Elements 

#include <bits/stdc++.h> 

pair<int,int> missingAndRepeating(vector<int> &arr, int n)
{
	// Write your code here
    pair<int, int>mani;
    int i=0;
    while(i<n){
        if(arr[i] != arr[arr[i]-1]){
            swap(arr[i], arr[arr[i]-1]);
        }else{
            i++;
        }
    }
    for(int i=0; i<n; i++){
        if(arr[i] != i+1){
            mani.first = i+1;
            mani.second = arr[i];
        }
    }
    return mani;
}
