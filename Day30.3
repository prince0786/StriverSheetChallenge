## Return Subset Sum Equals to K

#include<bits/stdc++.h>
void solve(int i, int k, int n, vector<int>&arr, vector<int>&ds, vector<vector<int>>&ans){
    if(i==n){
        if(k==0){
            ans.push_back(ds);
        }
        return;
    }
    else{
        ds.push_back(arr[i]);
        solve(i+1,k-arr[i],n, arr, ds, ans);
        
        ds.pop_back();
        
        solve(i+1,k,n,arr,ds,ans);
    }
}
vector<vector<int>> findSubsetsThatSumToK(vector<int> arr, int n, int k)
{
    // Write your code here.
    vector<int>ds;
    vector<vector<int>>ans;
    solve(0,k,n,arr,ds,ans);
    
    return ans;
}
