#Merge two sorted Array

#include <bits/stdc++.h> 

vector<int> ninjaAndSortedArrays(vector<int>& arr1, vector<int>& arr2, int m, int n) {
	// Write your code here.
    vector<int>ans;
    for(int i=0; i<m; i++){
        ans.push_back(arr1[i]);
    }
    for(int j=0; j<n; j++){
        ans.push_back(arr2[j]);
    }
    sort(ans.begin(), ans.end());
    return ans;
}
