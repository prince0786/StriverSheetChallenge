### Subset Sum II

#include<bits/stdc++.h>
void solve(int i, vector<int>sum, vector<int>arr, vector<vector<int>> &subsetSum){
    if(i == arr.size()){
        subsetSum.push_back(sum);
        return;
    }
    sum.push_back(arr[i]);
    solve(i+1,sum, arr, subsetSum);
    sum.pop_back();
    while(i+1 < arr.size() && arr[i]==arr[i+1]) i++;
    solve(i+1,sum, arr, subsetSum);
}
